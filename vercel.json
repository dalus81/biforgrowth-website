{
  "version": 2,
  "builds": [
    { "src": "api/contact-simple.js", "use": "@vercel/node" },
    { "src": "dist/public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/contact", "dest": "/api/contact-simple.js" },
    { "src": "/(assets/.+)", "dest": "/dist/public/$1" },
    { "src": "/(.*)", "dest": "/dist/public/index.html" }
  ]
}
